lse\n    }];\n    let search = ref('');\n    let cur_item = ref(null);\n    let popup1 = ref(false);\n    let popup2 = ref(false);\n    let new_item = ref(null);\n    function closePopUp() {\n      popup1.value = false;\n      popup2.value = false;\n      setTimeout(() => {\n        new_item.value = {\n          mname: \"\",\n          mtype: \"\",\n          imgurl: \"\",\n          price: 0,\n          available: \"\",\n          recommanded: \"\"\n        };\n      }, 300);\n    }\n    function showDialog1(item) {\n      cur_item.value = item;\n      popup1.value = true;\n    }\n    function showDialog2() {\n      popup2.value = true;\n    }\n    function updateMenu() {\n      if (cur_item.value.mname === \"\" || cur_item.value.mtype === \"\" || cur_item.value.recommanded === \"\" || cur_item.value.available === \"\" || cur_item.value.price === 0 || isNaN(cur_item.value.price)) {\n        alert(\"FAIL!\");\n      } else {\n        let menuDTO = {\n          'mno': cur_item.value.mno,\n          'mname': cur_item.value.mname,\n          'mtype': cur_item.value.mtype,\n          'price': cur_item.value.price,\n          'available': cur_item.value.available === \"가능\" ? \"1\" : \"0\",\n          'recommanded': cur_item.value.recommanded === \"O\" ? \"1\" : \"0\",\n          'imgurl': cur_item.value.imgurl\n        };\n        const formData = new FormData();\n        const json = JSON.stringify(menuDTO);\n        const blob = new Blob([json], {\n          type: \"application/json\"\n        });\n        formData.append(\"menuDTO\", blob);\n        if (document.getElementById(\"img-edit\").files[0] !== null) {\n          formData.append(\"file\", document.getElementById(\"img-edit\").files[0]);\n        }\n        axios.post('/api/admin/menu/update', formData, {\n          headers: {\n            \"Content-Type\": `multipart/form-data`\n          }\n        }).then(() => {\n          emit(\"refresh\");\n        });\n        closePopUp();\n      }\n    }\n    function insertMenu() {\n      if (new_item.value.mname === \"\" || new_item.value.mtype === \"\" || new_item.value.recommanded === \"\" || new_item.value.available0 === \"\" || new_item.value.price === 0 || isNaN(new_item.value.price)) {\n        alert(\"FAIL!\");\n      } else {\n        // axios.post('/admin/menu/update', {\n        //   'mname': new_item.value.mname,\n        //   'mtype': new_item.value.mtype,\n        //   'price': new_item.value.price,\n        //   'available': (new_item.value.available === \"가능\" ? \"1\" : \"0\"),\n        //   'recommanded': (new_item.value.recommanded === \"O\" ? \"1\" : \"0\"),\n        //   'imgurl': new_item.value.imgurl,\n        // }).then(()=>{emit(\"refresh\")});\n\n        let menuDTO = {\n          'mname': new_item.value.mname,\n          'mtype': new_item.value.mtype,\n          'price': new_item.value.price,\n          'available': new_item.value.available === \"가능\" ? \"1\" : \"0\",\n          'recommanded': new_item.value.recommanded === \"O\" ? \"1\" : \"0\",\n          'imgurl': new_item.value.imgurl\n        };\n        const formData = new FormData();\n        const json = JSON.stringify(menuDTO);\n        const blob = new Blob([json], {\n          type: \"application/json\"\n        });\n        formData.append(\"menuDTO\", blob);\n        if (document.getElementById(\"img\").files[0] !== null) {\n          formData.append(\"file\", document.getElementById(\"img\").files[0]);\n        }\n        axios.post('/api/admin/menu/update', formData, {\n          headers: {\n            \"Content-Type\": `multipart/form-data`\n          }\n        }).then(() => {\n          emit(\"refresh\");\n        });\n        closePopUp();\n      }\n    }\n    function confirmDelete(item, i) {\n      if (confirm('정말 삭제하시겠습니까?')) {\n        if (i == 1) {\n          axios.delete('/api/admin/menu/' + item.mno, {}).then(() => {\n            emit(\"refresh\");\n          });\n        } else if (i == 2) {\n          axios.delete('/api/admin/order/' + item.ono, {}).then(() => {\n            emit(\"refresh\");\n          });\n        }\n      }\n    }\n    const __returned__ = {\n      get currentTab() {\n        return currentTab;\n      },\n      set currentTab(v) {\n        currentTab = v;\n      },\n      emit,\n      props,\n      rules,\n      priceRules,\n      orderHeaders,\n      menuHeaders,\n      get search() {\n        return search;\n      },\n      set search(v) {\n        search = v;\n      },\n      get cur_item() {\n        return cur_item;\n      },\n      set cur_item(v) {\n        cur_item = v;\n      },\n      get popup1() {\n        return popup1;\n      },\n      set popup1(v) {\n        popup1 = v;\n      },\n      get popup2() {\n        return popup2;\n      },\n      set popup2(v) {\n        popup2 = v;\n      },\n      get new_item() {\n        return new_item;\n      },\n      set new_item(v) {\n        new_item = v;\n      },\n      closePopUp,\n      showDialog1,\n      showDialog2,\n      updateMenu,\n      insertMenu,\n      confirmDelete,\n      get axios() {\n        return axios;\n      },\n      ref,\n      computed,\n      onMounted,\n      AdminMainFooter\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n});","map":{"version":3,"names":["axios","ref","computed","onMounted","AdminMainFooter","__default__","name","new_item","value","mname","mtype","imgurl","price","available","recommanded","currentTab","emit","__emit","props","__props","isLoading","rules","priceRules","isNaN","orderHeaders","title","key","align","sortable","menuHeaders","search","cur_item","popup1","popup2","closePopUp","setTimeout","showDialog1","item","showDialog2","updateMenu","alert","menuDTO","mno","formData","FormData","json","JSON","stringify","blob","Blob","type","append","document","getElementById","files","post","headers","then","insertMenu","available0","confirmDelete","i","confirm","delete","ono"],"sources":["C:/Users/1/Desktop/vvv/frontend/src/components/AdminMainBody.vue"],"sourcesContent":["<template>\r\n  <v-card>\r\n    <v-tabs v-model=\"currentTab\" color=\"black\" align-tabs=\"center\" grow>\r\n      <v-tab :value=\"1\">메뉴 관리</v-tab>\r\n      <v-tab :value=\"2\">주문 내역</v-tab>\r\n    </v-tabs>\r\n    <div class=\"loading-progress\" v-if=\"isLoading\">\r\n      <v-progress-circular indeterminate></v-progress-circular>\r\n    </div>\r\n    <v-window v-if=\"!isLoading\" v-model=\"currentTab\">\r\n      <v-window-item v-for=\"n in 2\" :key=\"n\" :value=\"n\">\r\n        <v-container fluid>\r\n          <v-text-field v-model=\"search\" label=\"Search\" prepend-inner-icon=\"mdi-magnify\" single-line variant=\"outlined\"\r\n            hide-details style=\"width: 70%; display: inline-block;\"></v-text-field>\r\n\r\n          <v-data-table v-show=\"n == 2\" :items=\"myOrderData\" :headers=\"orderHeaders\" :search=\"search\"\r\n            :sort-by=\"[{ key: 'ono', order: 'desc' }]\">\r\n            <template v-slot:[`item.actions`]=\"{ item }\">\r\n              <v-icon v-if=\"false\" size=\"small\" class=\"me-2\" @click=\"console.log(item)\">\r\n                mdi-pencil\r\n              </v-icon>\r\n              <v-icon size=\"small\" @click=\"confirmDelete(item, 2)\">\r\n                mdi-delete\r\n              </v-icon>\r\n            </template>\r\n          </v-data-table>\r\n          <span>\r\n            <v-data-table v-show=\"n == 1\" :items=\"myMenuData\" :headers=\"menuHeaders\" :search=\"search\"\r\n            :sort-by=\"[{ key: 'mno', order: 'desc' }]\">\r\n              <template v-slot:[`item.actions`]=\"{ item }\">\r\n                <v-icon size=\"small\" class=\"me-2\" @click=\"showDialog1(item)\">\r\n                  mdi-pencil\r\n                </v-icon>\r\n                <v-icon size=\"small\" @click=\"confirmDelete(item, 1)\">\r\n                  mdi-delete\r\n                </v-icon>\r\n              </template>\r\n            </v-data-table>\r\n            <v-btn v-if=\"n == 1\" color=\"primary\" size=\"large\" dark class=\"mt-2\" @click=\"showDialog2()\">\r\n              메뉴 추가\r\n            </v-btn>\r\n          </span>\r\n        </v-container>\r\n      </v-window-item>\r\n    </v-window>\r\n\r\n    <!-- ********************다이얼로그***********************    -->\r\n\r\n    <v-dialog v-model=\"popup1\" persistent>\r\n      <v-form validate-on=\"submit lazy\" @submit.prevent>\r\n\r\n        <v-card width=\"768\" style=\"margin: auto;\">\r\n          <v-card-text>\r\n            <v-container class=\"dialog-container\">\r\n              <v-row>\r\n                <v-col cols=\"4\">\r\n                  <v-text-field :rules=\"rules\" v-model=\"cur_item.mname\" label=\"이름\" required></v-text-field>\r\n                </v-col>\r\n                <v-col cols=\"4\">\r\n                  <v-text-field :rules=\"[...rules, ...priceRules]\" v-model=\"cur_item.price\" label=\"가격(￦)\"\r\n                    required></v-text-field>\r\n                </v-col>\r\n              </v-row>\r\n              <v-row>\r\n                <v-col cols=\"4\">\r\n                  <v-select :rules=\"rules\" v-model=\"cur_item.mtype\" :items=\"['main', 'side', 'drink']\" label=\"타입\"\r\n                    required></v-select>\r\n                </v-col>\r\n                <v-col cols=\"4\">\r\n                  <v-select :rules=\"rules\" v-model=\"cur_item.available\" :items=\"['가능', '불가']\" label=\"이용\"\r\n                    required></v-select>\r\n                </v-col>\r\n                <v-col cols=\"4\">\r\n                  <v-select :rules=\"rules\" v-model=\"cur_item.recommanded\" :items=\"['O', 'X']\" label=\"추천\"\r\n                    required></v-select>\r\n                </v-col>\r\n              </v-row>\r\n              <v-row>\r\n                <v-file-input id=\"img-edit\" label=\"메뉴 사진\" accept=\"image/jpg, image/jpeg, image/png, image/gif\"></v-file-input>\r\n              </v-row>\r\n            </v-container>\r\n          </v-card-text>\r\n\r\n          <v-card-actions>\r\n            <v-spacer></v-spacer>\r\n            <v-btn color=\"blue-darken-1\" variant=\"text\" @click=\"closePopUp\">\r\n              Close\r\n            </v-btn>\r\n            <v-btn type=\"submit\" color=\"blue-darken-1\" variant=\"text\" @click=\"updateMenu\">\r\n              Save\r\n            </v-btn>\r\n          </v-card-actions>\r\n        </v-card>\r\n      </v-form>\r\n\r\n    </v-dialog>\r\n\r\n    <!-- **************************************************2****************************************************** -->\r\n    <v-dialog v-model=\"popup2\" persistent>\r\n      <v-form validate-on=\"submit lazy\" @submit.prevent>\r\n\r\n        <v-card width=\"768\" style=\"margin: auto;\">\r\n          <v-card-text>\r\n            <v-container class=\"dialog-container\">\r\n              <v-row>\r\n                <v-col cols=\"4\">\r\n                  <v-text-field v-model=\"new_item.mname\" :rules=\"rules\" label=\"이름\" required></v-text-field>\r\n                </v-col>\r\n                <v-col cols=\"4\">\r\n                  <v-text-field v-model=\"new_item.price\" type=\"number\" :rules=\"[...rules, ...priceRules]\" label=\"가격(￦)\"\r\n                    required></v-text-field>\r\n                </v-col>\r\n              </v-row>\r\n              <v-row>\r\n                <v-col cols=\"4\">\r\n                  <v-select v-model=\"new_item.mtype\" :rules=\"rules\" :items=\"['main', 'side', 'drink']\" label=\"타입\"\r\n                    required></v-select>\r\n                </v-col>\r\n                <v-col cols=\"4\">\r\n                  <v-select v-model=\"new_item.available\" :rules=\"rules\" :items=\"['가능', '불가']\" label=\"이용\"\r\n                    required></v-select>\r\n                </v-col>\r\n                <v-col cols=\"4\">\r\n                  <v-select v-model=\"new_item.recommanded\" :rules=\"rules\" :items=\"['O', 'X']\" label=\"추천\"\r\n                    required></v-select>\r\n                </v-col>\r\n              </v-row>\r\n              <v-row>\r\n                <v-file-input id=\"img\" label=\"메뉴 사진\" accept=\"image/jpg, image/jpeg, image/png, image/gif\"></v-file-input>\r\n              </v-row>\r\n            </v-container>\r\n          </v-card-text>\r\n\r\n          <v-card-actions>\r\n            <v-spacer></v-spacer>\r\n            <v-btn color=\"blue-darken-1\" variant=\"text\" @click=\"closePopUp\">\r\n              Close\r\n            </v-btn>\r\n            <v-btn type=\"submit\" color=\"blue-darken-1\" variant=\"text\" @click=\"insertMenu\">\r\n              Save\r\n            </v-btn>\r\n          </v-card-actions>\r\n        </v-card>\r\n      </v-form>\r\n\r\n    </v-dialog>\r\n\r\n\r\n  </v-card>\r\n  <admin-main-footer></admin-main-footer>\r\n</template>\r\n\r\n<script setup>\r\nimport axios from 'axios';\r\nimport { ref, defineProps, computed, onMounted, defineEmits } from 'vue'\r\nimport AdminMainFooter from './AdminMainFooter.vue';\r\n\r\nonMounted(() => {\r\n  new_item.value = {\r\n    mname: \"\",\r\n    mtype: \"\",\r\n    imgurl: \"\",\r\n    price: \"\",\r\n    available: \"\",\r\n    recommanded: \"\",\r\n  };\r\n});\r\n\r\nlet currentTab = ref(null)\r\n\r\n\r\nconst emit = defineEmits(['refresh']);\r\n\r\n\r\n\r\n\r\n\r\nconst props = defineProps(['myMenuData', 'myOrderData', 'isLoading']);\r\n\r\n\r\ncomputed(() => { return props.isLoading });\r\n\r\nconst rules = [\r\n  value => {\r\n    if (value) return true;\r\n    return 'invalid value';\r\n  }]\r\n\r\nconst priceRules = [\r\n  value => {\r\n    if (!isNaN(value)) return true;\r\n    return 'invalid value'\r\n\r\n  }]\r\n\r\n\r\n\r\nconst orderHeaders = [\r\n  { title: '주문 번호', key: 'ono', align: 'center' },\r\n  { title: '주문 내용', key: 'content', align: 'center' },\r\n  { title: '가격(￦)', key: 'totalPrice', align: 'center' },\r\n  { title: '주문 시각', key: 'regDate', align: 'center' },\r\n